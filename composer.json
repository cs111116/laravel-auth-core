{
    "name": "garycodingteam/laravel-auth-module",
    "type": "library",
    "description": "Reusable Laravel Auth API module",
    "license": "MIT",
    "authors": [
        {
            "name": "gary",
            "email": "garycodingwork@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GaryCodingTeam\\AuthModule\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "garycodingteam\\AuthModule\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
